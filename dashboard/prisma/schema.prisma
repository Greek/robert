generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model TLXReport {
  id                       String  @id() @default(auto()) @map("_id") @db.ObjectId
  name                     String
  icon                     String
  icon_hash                String?
  owner                    Int?
  approximate_member_count Int?
}
